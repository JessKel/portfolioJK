<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" href="{{ asset('build/images/favicon.ico') }}">
        <title>
            {% block title %}Mon Portfolio dashboard{% endblock %}
        </title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
        {% endblock %}
    </head>
    <body class="body-admin" id="body-jk-admin" style="background-image:url('{{ asset('build/images/fondecranJK.png') }}')">
        <header class="header-admin" id="header-admin">
            {% include 'includes/_navbar_dashboard.html.twig'%}
        </header>
        {% include 'includes/_aside_dashboard.html.twig'%}
        <main class="container main-admin">
            {% block body %}{% endblock %}
        </main>
    </body>
</html>
